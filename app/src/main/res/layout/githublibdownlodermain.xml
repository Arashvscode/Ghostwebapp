<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:paddingLeft="8dp"
		android:paddingTop="9dp"
		android:paddingRight="8dp"
		android:paddingBottom="8dp"
		android:background="#1F1B1C"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="4dp"
			android:padding="4dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="4dp"
				android:text="Write lib dependency"
				android:textSize="18sp"
				android:textColor="#E0E0E0"
				android:layout_weight="1"
				android:singleLine="true"
				android:focusable="false"
				
				android:fontFamily="@font/GhostFont" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="4dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<ProgressBar
				android:id="@+id/progressbar1"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="4dp"
				android:layout_marginBottom="4dp"
				android:padding="8dp"
				android:indeterminate="false"
				style="?android:progressBarStyleHorizontal"
				android:focusable="false" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear4"
			android:layout_width="match_parent"
			android:layout_height="50dp"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="4dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<com.google.android.material.textfield.TextInputLayout
				android:id="@+id/textinputlayout1"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="1dp"
				android:layout_marginRight="4dp"
				android:layout_marginBottom="1dp"
				android:orientation="vertical"
				style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
				<EditText
					android:id="@+id/edittext1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:textSize="14sp"
					android:textColor="#EEEEEE"
					android:hint="Example: com.anjlab.android.iab.v3:library:1.0.44"
					android:textColorHint="#EEEEEE"
					android:lines="1"
					android:singleLine="true"
					
					android:focusable="true"
					
					android:fontFamily="@font/GhostFont" />
			</com.google.android.material.textfield.TextInputLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear5"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="4dp"
			android:padding="4dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingTop="2dp"
				android:paddingBottom="5dp"
				android:text="شما می توانید وابستگی یک کتابخانه را در Github، معمولاً در فایل Readme پیدا کنید.  با این حال، لطفاً توجه داشته باشید که این سیستم Gradle نیست، بنابراین وابستگی‌های درونی یک کتابخانه را دانلود نمی‌کند.  اگر کتابخانه ای که دانلود می کنید وابستگی های درونی دارد، باید آنها را جداگانه دانلود کنید پیشنهاد میشه با CodeAssist کتابخانه ها را دانلود کنید همراه با وابسته گی ها که خیلی کتابخانه های کمی نیازی به وابسته گی خاصی دارن توجه کنید اگر کتابخانه‌ ای نصب نشد پیشهاد ما برنامه شکن است\nابتدا برنامه شکن را نصب کنید سپس فعال سازی و سپس دانلود کتابخانه "
				android:textSize="14sp"
				android:textColor="#E0E0E0"
				android:focusable="false"
				
				android:fontFamily="@font/GhostFont" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear6"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginRight="4dp"
			android:paddingLeft="2dp"
			android:paddingTop="2dp"
			android:paddingRight="2dp"
			android:paddingBottom="4dp"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/linear8"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="2dp"
				android:layout_marginRight="2dp"
				android:padding="2dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:layout_weight="1"
				android:background="@drawable/back">
				<TextView
					android:id="@+id/textview4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="   شروع    "
					android:textSize="14sp"
					android:textColor="#FFFFFF"
					android:focusable="false"
					android:singleLine="true"
					
					android:fontFamily="@font/GhostFont" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear9"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="2dp"
				android:layout_marginRight="2dp"
				android:padding="2dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:layout_weight="1"
				android:background="@drawable/back">
				<TextView
					android:id="@+id/textview5"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="  متوقف  "
					android:textSize="14sp"
					android:textColor="#FFFFFF"
					android:focusable="false"
					android:singleLine="true"
					
					android:fontFamily="@font/GhostFont" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear10"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="2dp"
				android:layout_marginRight="2dp"
				android:padding="2dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:layout_weight="1"
				android:background="@drawable/back">
				<TextView
					android:id="@+id/textview6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="ازسرگیری"
					android:textSize="14sp"
					android:textColor="#FFFFFF"
					android:focusable="false"
					android:singleLine="true"
					
					android:fontFamily="@font/GhostFont" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="2dp"
				android:layout_marginRight="2dp"
				android:padding="2dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:layout_weight="1"
				android:background="@drawable/back">
				<TextView
					android:id="@+id/textview7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="  لغو کنید  "
					android:textSize="14sp"
					android:textColor="#FFFFFF"
					android:focusable="false"
					android:singleLine="true"
					android:fontFamily="@font/GhostFont" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
